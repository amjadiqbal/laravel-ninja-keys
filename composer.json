{
  "name": "amjadiqbal/laravel-ninja-keys",
  "description": "Laravel integration for the ninja-keys web component with a fluent PHP API and Blade directive.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "AmjadIqbal"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "AmjadIqbal\\NinjaKeys\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AmjadIqbal\\NinjaKeys\\NinjaKeysServiceProvider"
      ],
      "aliases": {
        "NinjaKeys": "AmjadIqbal\\NinjaKeys\\Facades\\NinjaKeys"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "AmjadIqbal\\NinjaKeys\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
